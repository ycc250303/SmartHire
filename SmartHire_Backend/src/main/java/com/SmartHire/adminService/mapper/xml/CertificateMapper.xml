<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.SmartHire.adminService.mapper.CertificateMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.SmartHire.adminService.model.Certificate">
        <id column="id" property="id" />
        <result column="job_seeker_id" property="jobSeekerId" />
        <result column="certificate_name" property="certificateName" />
        <result column="certificate_url" property="certificateUrl" />
        <result column="created_at" property="createdAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, job_seeker_id, certificate_name, certificate_url, created_at
    </sql>

</mapper>
